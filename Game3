local library = loadstring(game:HttpGet("https://github.com/GoHamza/AppleLibrary/blob/main/main.lua?raw=true"))()


local window = library:init("Apple X", true, Enum.KeyCode.RightShift, true)

local sectionA = window:Section("Characters")

sectionA:Button("Find all Characters", function() window:Notify("Button triggered !", "I recommend turning off the sound in the game settings or lowering the volume before pressing this button.", "Execute", "rbxassetid://12608259004",
   function()
local player = game:GetService("Players").LocalPlayer
local hrp = player.Character or player.CharacterAdded:Wait()
hrp = hrp:WaitForChild("HumanoidRootPart")

local mdl = workspace:FindFirstChild("ToFind")
if not mdl then return end

for _,v in ipairs(mdl:GetDescendants()) do
    if v:IsA("BasePart") then
        pcall(function()
            v.CFrame = hrp.CFrame + Vector3.new(0,2,0)
        end)
    end
end
   end)
end)

sectionA:Select()
